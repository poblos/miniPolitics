<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.demo.StartView" spacing="100" alignment="CENTER">
    <Label text="THE MAPS" fx:id="title">
    </Label>
    <Label alignment="CENTER">
        <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" scaleX="5" scaleY="5">
                <Image url="@menu_icons/planet.png"/>
            </ImageView>
        </graphic>
    </Label>
    <VBox spacing="20.0" alignment="CENTER">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </padding>
        <Button fx:id="kaButton" onAction="#onKachakonyaButtonClick" text="Start the game as Kachakonya"/>
        <Button fx:id="grButton" onAction="#onGraoniaButtonClick" text="Start the game as Graonia"/>
    </VBox>
</VBox>
